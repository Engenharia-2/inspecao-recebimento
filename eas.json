{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL_DEVELOPMENT": "http://192.168.0.10:3001"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL_PRODUCTION": "http://192.168.0.10:3001"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL_PRODUCTION": "http://192.168.0.10:3001"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
